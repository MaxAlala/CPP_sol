import java.util.Scanner;

public class Number {
	public static void main(String args[]) {

		int a = 1; 
		int b = 1000; 
		int num = -1;
		int rnd =  a + (int) (Math.random() * b); 
		System.out.println(rnd);
		while(num!=rnd) {
			Scanner sc = new Scanner(System.in);
			num = sc.nextInt();

		}
		if(num==rnd)System.out.println("You won! Number is: "+ rnd);

	}
}
