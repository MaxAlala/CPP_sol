
public class Rectangle {
	private int x1;
	private int x2;
	private int x3;
	private int x4;
	private int y1;
	private int y2;
	private int y3;
	private int y4;

	Rectangle(int x1, int x2, int x3, int x4, int y1, int y2, int y3, int y4){
		this.x1 = x1;
		this.x2 = x2;
		this.x3 = x3;
		this.x4 = x4;
		this.y1 = y1;
		this.y2 = y2;
		this.y3 = y3;
		this.y4 = y4;
	}



	public static void main(String args[]) {

	}

	void moving(int x, int y){
		x1+=x;
		x2+=x;
		x3+=x;
		x4+=x;
		y1+=y;
		y2+=y;
		y3+=y;
		y4+=y;

	}

	void changeSize(int x, int y) {
		if(x2<=x3 & x2<=x4 & x2 <= x1) {

			x1+=x;
			x3+=x;
			x4+=x;
			y1+=y;
			y3+=y;
			y4+=y;

		} else
			if(x1<=x3 & x1<=x4 & x1 <= x2) {
				x2+=x;
				x3+=x;
				x4+=x;
				y2+=y;
				y3+=y;
				y4+=y;   
			} else
				if(x3<=x1 & x3<=x4 & x3 <= x2) {
					x2+=x;
					x1+=x;
					x4+=x;
					y2+=y;
					y1+=y;
					y4+=y;   
				} else
					if(x4<=x3 & x4<=x1 & x4 <= x2) {
						x2+=x;
						x1+=x;
						x3+=x;
						y2+=y;
						y1+=y;
						y3+=y;   
					}
	}

	void minimal2Xrectangle(){
		
	if(x1 != 0) {
		x1+=x1;
		x2+=x2;
	} else
		if(x3 != 0) {
			x3+=x3;
			x4+=x4;
		}
		
		
		 


	}
}
