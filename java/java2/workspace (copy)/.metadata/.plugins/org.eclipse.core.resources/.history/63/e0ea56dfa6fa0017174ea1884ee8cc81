
public class Sort {
	public static void main(String args[]) {
		System.out.println(sortWords("acbqweqw","abcee"));	
	}


	     void sortAr(String[] ar){
	    	 int n = ar.length;
	    	 String bufStr;
	    	 String [] ar2 = new String[n];
	    	 
	    	 for(int i = 0; i < ar.length; i++) 
	    		 for(int j = 0; j < ar.length; j++) {
	    			 
	    			 if (ar.length == 1) break;
	    			 boolean buf = sortWords(ar[i], ar[i+1]);
	    			 if(buf == false) {
	    				 bufStr = ar[i+1];
	    				 ar[i+1] = ar[i];
	    				 ar[i] = bufStr; 
	    			 }     
	            }
	     }
	
	static boolean sortWords(String x, String y){
		if((x.length()>1)&&(y.length()>1)) {

			if(x.charAt(0) > y.charAt(0)) return false;
			else if(x.charAt(0) < y.charAt(0)) return true;
			else if(x.charAt(0) == y.charAt(0)) 
				if(x.charAt(1) < y.charAt(1)) return true;
				else return false;


		}else if(x.charAt(0)<y.charAt(0)) return true;
		else return false;


		return false; 
	}

}
