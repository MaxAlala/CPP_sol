	static String sortWords(String x, String y){
		if((x.length()>1)&&(y.length()>1)) {

			if(x.charAt(0) > y.charAt(0)) return y;
			else if(x.charAt(0) < y.charAt(0)) return x;
			else if(y.charAt(0) == x.charAt(0)) 
				if(y.charAt(1) < x.charAt(1)) return y;
				else return x;


		}else if(x.charAt(0)<y.charAt(0)) return x;
		else return y;


		return "something wrong!"; 
	}

}
