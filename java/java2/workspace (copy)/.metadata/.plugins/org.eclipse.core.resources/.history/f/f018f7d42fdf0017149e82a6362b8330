import java.util.Scanner;

public class Number {
public static void main(String args[]) {
	
	int a = 0; // Начальное значение диапазона - "от"
    int b = 10; // Конечное значение диапазона - "до"
    int num = -1;
int rnd =  a + (int) (Math.random() * b); 	
while(num!=rnd) {
	Scanner sc = new Scanner(System.in);
	 num = sc.nextInt();
	
}
	
	
}
}
